ou're not using actual ratings for testing)\n","dummy_ratings = [1] * len(test_df)\n","testset = list(zip(test_df['session_id'], test_df['song_id'], dummy_ratings))\n","\n","# Predictions for the test set\n","predictions = model.test(testset)\n","\n","# Extract the top 5 predictions for each session\n","top5_predictions = {}\n","for session_id, song_id, _, predicted_listening_order, _ in predictions:\n","    if session_id not in top5_predictions:\n","        top5_predictions[session_id] = []\n","    top5_predictions[session_id].append((song_id, predicted_listening_order))\n","\n","# Sort and extract the top 5 for each session\n","for session_id, predictions_list in top5_predictions.items():\n","    predictions_list.sort(key=lambda x: x[1])\n","    top5_predictions[session_id] = [song_id for song_id, _ in predictions_list[:5]]\n","\n","# Create a DataFrame for submission\n","submission_df = pd.DataFrame(top5_predictions.items(), columns=['session_id', 'top5'])\n","submission_df[['top1', 'top2', 'top3', 'top4', 'top5']] = pd.DataFrame(submission_df['top5'].tolist(), index=submission_df.index)\n","\n","other_columns = [col for col in submission_df.columns if col != 'top5']\n","\n","# Reorder columns\n","new_order = other_columns + ['top5']\n","submission_df = submission_df[new_order]\n","\n","# Save submission file\n","submission_df.to_csv('/content/submission.csv', index=False)"]}]}