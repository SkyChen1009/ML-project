trix = np.dot(W, H)\n","\n","            # Get top 5 songs for each session in the test set\n","            test_sessions = batch_df['session_id'].unique()\n","            top5_songs = []\n","\n","            for session_id in test_sessions:\n","                if session_id in user_item_matrix.index:\n","                    session_index = user_item_matrix.index.get_loc(session_id)\n","                    top_indices = np.argsort(predicted_matrix[session_index, :])[-5:][::-1]\n","\n","                    # Map indices back to 'song_id' values\n","                    top_songs = user_item_matrix.columns[top_indices].tolist()\n","\n","                    top5_songs.append(top_songs)\n","                else:\n","                    top5_songs.append([])\n","\n","            # Yield the results for the current batch\n","            yield test_sessions, top5_songs\n","\n","# Sample a fraction of the training data\n","train_df = train_df.sample(frac=0.01, random_state=42)\n","\n","# Merge datasets\n","merged_df = pd.concat([train_df, test_df])\n","\n","# Create the data generator\n","batch_size = 10\n","generator = data_generator(merged_df, batch_size)\n","\n","# Number of epochs\n","num_epochs = 1  # Adjust as needed\n","\n","# Create an empty DataFrame to store results\n","submission_df = pd.DataFrame(columns=['session_id', 'top1', 'top2', 'top3', 'top4', 'top5'])\n","\n","# Number of batches per epoch\n","num_batches = len(merged_df) // batch_size\n","\n","# Iterate over epochs\n","for epoch in range(num_epochs):\n","    print(f\"Epoch {epoch + 1}/{num_epochs}\")\n","\n","    # Iterate over batches\n","    for _ in tqdm(range(num_batches)):\n","        test_sessions, top5_songs = next(generator)\n","\n","        # Create the submission DataFrame for the current batch\n","        batch_submission_df = pd.DataFrame({\n","            'session_id': test_sessions,\n","            'top1': [indices[0] if indices else -1 for indices in top5_songs],\n","            'top2': [indices[1] if len(indices) > 1 else -1 for indices in top5_songs],\n","            'top3': [indices[2] if len(indices) > 2 else -1 for indices in top5_songs],\n","            'top4': [indices[3] if len(indices) > 3 else -1 for indices in top5_songs],\n","            'top5': [indices[4] if len(indices) > 4 else -1 for indices in top5_songs],\n","        })\n","\n","        # Append the results to the main submission DataFrame\n","        submission_df = pd.concat([submission_df, batch_submission_df], ignore_index=True)\n","\n","# Export the final submission file\n","submission_df.to_csv(\"submission.csv\", index=False)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"GBtlqH4UyrPi","executionInfo":{"status":"ok","timestamp":1703387644311,"user_tz":-480,"elapsed":393923,"user":{"displayName":"Sky Sky Sky","userId":"13772767823634088015"}},"outputId":"ad4c7c96-c7a2-44d5-c299-57e3d21c8afe"},"execution_count":16,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch 1/1\n"]},{"output_type":"stream","name":"stderr","text":["  0%|          | 15/14878 [00:01<21:49, 11.35it/s]/usr/local/lib/python3.10/dist-packages/sklearn/decomposition/_nmf.py:1665: ConvergenceWarning: Maximum number of iterations 200 reached. Increase it to improve convergence.\n","  warnings.warn(\n","  0%|          | 52/14878 [00:03<13:36, 18.16it/s]/usr/local/lib/python3.10/dist-packages/sklearn/decomposition/_nmf.py:1665: ConvergenceWarning: Maximum number of iterations 200 reached. Increase it to improve convergence.\n","  warnings.warn(\n","  0%|          | 73/14878 [00:03<07:50, 31.49it/s]/usr/local/lib/python3.10/dist-packages/sklearn/decomposition/_nmf.py:1665: ConvergenceWarning: Maximum number of iterations 200 reached. Increase it to improve convergence.\n","  warnings.warn(\n","  2%|▏         | 280/14878 [00:07<04:06, 59.26it/s]/usr/local/lib/python3.10/dist-packages/sklearn/decomposition/_nmf.py:1665: ConvergenceWarning: Maximum number of iterations 200 reached. I